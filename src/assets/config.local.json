{
  "system": {
    "applicationName": "APP_NAME",
    "applicationUrl": "http://localhost:4200"
  },
  "seo": {
    "defaultPageTitle": "DEFAULT_TITLE",
    "pageTitlePositioning": 10,
    "pageTitleSeparator": " | ",
    "defaultMetaDescription": "DEFAULT_META_DESCRIPTION"
  },
  "i18n": {
    "defaultLanguage": {
      "code": "en",
      "name": "English",
      "culture": "en-US"
    },
    "availableLanguages": [
      {
        "code": "en",
        "name": "English",
        "culture": "en-US"
      },
      {
        "code": "ru",
        "name": "Русский",
        "culture": "ru-RU"
      }
    ],
    "useLocalizedRoutes": true
  },
  "routes": {
    "en": {
      "ROOT.ABOUT": "about",
      "ROOT.ABOUT.US": "us",
      "CHANGE_LANGUAGE": "change-language"
    },
    "ru": {
      "ROOT.ABOUT": "О сервисе",
      "ROOT.ABOUT.US": "О нас",
      "CHANGE_LANGUAGE": "изменить язык"
    }
  },
  "backend": {
    "baseBrowserUrl": "http://localhost:4200",
    "baseServerUrl": "http://localhost:4000",
    "baseAuthUrl": "http://localhost:3005/auth",
    "baseApiUrl": "http://localhost:3005/api",
    "auth": {
      "auth": {
        "endpoint": "{baseAuthUrl}/login"
      }
    },
    "admin": {
      "user": {
        "endpoint": "{baseApiUrl}/user"
      }
    }
  }
}
